
public class Rectangle extends Shape{
	
	
	protected double length;
	protected double width;
	
	public Rectangle(boolean filled, String color,double length,double width) {
		super(filled, color);
		
		this.color=color;
		this.filled=filled;
		this.length=length;
		this.width=width;
	}

	@Override
	public String toString() {
		return "Rectangle [ color=\" + color + \", filled=\" + filled + \"]\"length=" + length + ", width=" + width;
	}
	
	double getArea()
	{
		return length*width;
		
		
	}
	
	
	
	double getPerimeter()
	{
		return 2*(length+width);
		
		
	}
	
	
	
	
	
	
	
	
	

}
