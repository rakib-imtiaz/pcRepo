
public class Fruit {
	
	private String color;
	private String seed;
	private String season;
	
	static final String SUMMER="SUMMER";
	static final String WINTER="WINTER";
	static final String AUTUMN="AUTUMN";
	
	
	
	/*
	 * public static String getSummer() { return SUMMER; } public static String
	 * getWinter() { return WINTER; } public static String getAutumn() { return
	 * AUTUMN; }
	 */
	
	public String getColor() {
		return color;
	}
	public void setColor(String color) {
		this.color = color;
	}
	public String getSeed() {
		return seed;
	}
	public void setSeed(String seed) {
		this.seed = seed;
	}
	public String getSeason() {
		return season;
	}
	public void setSeason(String season) {
		this.season = season;
	}
	
}
